{
  "name": "Print Gmail without logo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Print Gmail content without the Gmail logo in the header",
  "homepage_url": "https://github.com/zhangsu/print-gmail-without-logo",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  }
}
