{
  "name": "Print Gmail without logo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Print Gmail content without the Gmail logo in the header.",
  "homepage_url": "https://github.com/zhangsu/print-gmail-without-logo",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/mail/*&view=pt*"],
      "js": ["js/hide_logo.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "css/hide_logo.css"
  ]
}
